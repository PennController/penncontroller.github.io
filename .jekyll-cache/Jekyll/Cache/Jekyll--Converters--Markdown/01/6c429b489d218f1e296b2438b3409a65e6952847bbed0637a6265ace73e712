    <span class="dl">"</span><span class="s2">Type,Sentence</span><span class="se">\n</span><span class="dl">"</span><span class="o">+</span>           <span class="c1">// Column names (mind the \n)</span>
    <span class="dl">"</span><span class="s2">practice,Hello world!</span><span class="se">\n</span><span class="dl">"</span><span class="o">+</span>   <span class="c1">// First row (mind the \n)</span>
    <span class="dl">"</span><span class="s2">test,Bye world!</span><span class="dl">"</span>            <span class="c1">// Second row</span>
<span class="p">);</span>

<span class="nx">Template</span><span class="p">(</span> <span class="dl">"</span><span class="s2">myTable</span><span class="dl">"</span> <span class="p">,</span> <span class="nx">row</span> <span class="o">=&gt;</span> 
    <span class="nx">newTrial</span><span class="p">(</span>
        <span class="nx">newText</span><span class="p">(</span><span class="dl">"</span><span class="s2">type</span><span class="dl">"</span><span class="p">,</span> <span class="nx">row</span><span class="p">.</span><span class="nx">Type</span><span class="p">)</span>
            <span class="p">.</span><span class="nx">print</span><span class="p">()</span>
        <span class="p">,</span>
        <span class="nx">newButton</span><span class="p">(</span><span class="dl">"</span><span class="s2">sentence</span><span class="dl">"</span><span class="p">,</span> <span class="nx">row</span><span class="p">.</span><span class="nx">Sentence</span><span class="p">)</span>
            <span class="p">.</span><span class="nx">print</span><span class="p">()</span>
